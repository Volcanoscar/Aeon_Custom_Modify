+package com.android.settings;
+
+import android.content.Context;
+import android.preference.Preference;
+import android.util.AttributeSet;
+import android.view.LayoutInflater;
+import android.view.View;
+import android.view.View.OnClickListener;
+import android.view.ViewGroup;
+import android.widget.CompoundButton;
+import android.widget.ImageView;
+import android.widget.RadioButton;
+import android.widget.TextView;
+import android.view.ViewGroup;
+
+
+
+import com.android.settings.R;
+import android.os.SystemProperties;
+import android.util.Log;
+
+public class HardKeyPreference extends Preference implements
+        CompoundButton.OnCheckedChangeListener/*, OnClickListener*/ {
+
+    private static final String TAG = "HardKeyPreference";
+    private static final String HARDKEY_NORMAL_KEY = "hardkey_normal_mode";
+    private static final String HARDKEY_REVERSAL_KEY = "hardkey_reversal_mode";
+    private static CompoundButton sCurrentChecked = null;
+    private static String sActiveKey = null;
+
+    private String mPreferenceTitle = null;
+    private String mPreferenceSummary = null;
+
+    private TextView mTextView = null;
+    private TextView mSummary = null;
+    private RadioButton mCheckboxButton = null;
+    private	ImageView mHardKeyImage = null;
+
+    private Context mContext;
+    private String mKey;
+    private LayoutInflater mInflater;
+
+    private View mLayout;
+    private HardKeySettingsEX.OnHardKeyClickListener	mOnHardKeyClickListener;
+
+
+    /**
+     * HardKeyPreference construct method
+     * 
+     * @param context
+     *            the context which is associated with, through which it can
+     *            access the theme and the resources
+     * @param attrs
+     *            the attributes of XML tag that is inflating the preference
+     * @param defStyle
+     *            the default style to apply to the preference
+     */
+    public HardKeyPreference(Context context, AttributeSet attrs,
+            int defStyle) {
+        super(context, attrs, defStyle);
+
+        mContext = context;
+
+        mInflater = (LayoutInflater) mContext
+                .getSystemService(Context.LAYOUT_INFLATER_SERVICE);
+
+        // get the title from hardkey_settings.xml
+        if (super.getTitle() != null) {
+            mPreferenceTitle = super.getTitle().toString();
+        }
+
+        // get the summary from hardkey_settings.xml
+        if (super.getSummary() != null) {
+            mPreferenceSummary = super.getSummary().toString();
+        }
+        mKey = getKey();
+    }
+
+    /**
+     * HardKeyPreference construct method
+     * 
+     * @param context
+     *            the context which is associated with, through which it can
+     *            access the theme and the resources
+     * @param attrs
+     *            attrs the attributes of XML tag that is inflating the
+     *            preference
+     */
+    public HardKeyPreference(Context context, AttributeSet attrs) {
+        this(context, attrs, 0);
+    }
+
+    /**
+     * HardKeyPreference construct method
+     * 
+     * @param context
+     *            which is associated with, through which it can access the
+     *            theme and the resources
+     */
+    public HardKeyPreference(Context context) {
+        this(context, null);
+    }
+
+
+    /**
+     * Gets the view that will be shown in the PreferenceActivity
+     * 
+     * @param parent
+     *            The parent that this view will eventually be attached
+     * @return the preference object
+     */
+    @Override
+    public View onCreateView(ViewGroup parent) {
+        Log.d(TAG , "onCreateView  " );
+        mLayout = mInflater.inflate(R.layout.hard_key_item, null); 
+
+        mCheckboxButton = (RadioButton)mLayout.findViewById(R.id.radiobutton);
+        mTextView = (TextView) mLayout.findViewById(R.id.hardkey_layout_text);
+        mSummary = (TextView) mLayout.findViewById(R.id.hardkey_layout_summary);
+        
+        if (mCheckboxButton != null) {
+            mCheckboxButton.setOnClickListener(new OnClickListener() {
+
+                public void onClick(View v) {
+                    // TODO Auto-generated method stub
+                    Log.d(TAG , "onClick " + getKey());
+
+                    if (!mCheckboxButton.equals(sCurrentChecked)) {
+                        if (sCurrentChecked != null) {
+                            sCurrentChecked.setChecked(false);
+                            mCheckboxButton.setChecked(true);
+                            sCurrentChecked = mCheckboxButton;
+                            sActiveKey = getKey();
+
+                            if (sActiveKey != null && (HARDKEY_REVERSAL_KEY.equals(sActiveKey))) {
+                                Log.d(TAG,"SystemProperties set 1" );
+                                SystemProperties.set("persist.sys.input.changemode", "1");
+                            }else{
+                                SystemProperties.set("persist.sys.input.changemode", "0");
+                            }
+							
+							if(mOnHardKeyClickListener!=null){
+	                               Log.d(TAG,"mOnHardKeyClickListener onClick" );							
+								 mOnHardKeyClickListener.onClick();
+							}
+							
+                        }
+                    } else {
+                        Log.d(TAG,
+                                "Click the active profile, do nothing return");
+                    }
+                }
+            });
+            
+
+            mCheckboxButton.setChecked(isChecked());
+            if (isChecked()) {
+                setChecked();
+            }	
+        }
+		
+		/*fix it later*/
+        if (mPreferenceTitle != null && mTextView != null) {
+            mTextView.setText(mPreferenceTitle);
+        } else {
+            Log.d(TAG , "PreferenceTitle is null");
+        }
+
+        if (mPreferenceSummary != null && mSummary != null) {
+            mSummary.setText(mPreferenceSummary);
+        } else {
+            Log.d(TAG , "mPreferenceSummary is null");
+        }
+
+        if (mSummary != null) {
+			mSummary.setVisibility(View.GONE);
+        }
+
+		if(mTextView!=null){
+			mTextView.setVisibility(View.GONE);
+        }
+			
+        mHardKeyImage = (ImageView) mLayout.findViewById(R.id.hardkeyImage);
+
+		
+        dynamicShowImage();
+
+        return mLayout;
+    }
+
+	public void setHardKeyImage(int resID){
+        Log.d(TAG, mKey + " setHardKeyImage");
+        if(mHardKeyImage!=null){
+             mHardKeyImage.setImageResource(resID);
+        }
+	}
+
+    /**
+     * dynamic show the preference summary according to the hard key settings eg: 
+     * / Back Home Menu -> Menu Home Back
+     * 
+     */
+    public void dynamicShowImage() {
+        Log.d(TAG, mKey + " dynamicShowImage mKey =" +mKey +" mHardKeyImage ="+  mHardKeyImage +" isChecked ="+isChecked() );
+
+		//mHardKeyImage.setLayoutParams(new ViewGroup.LayoutParams(200, 100));
+		if(mKey != null && mHardKeyImage!=null){
+        if ((HARDKEY_REVERSAL_KEY.equals(mKey))) {
+			if(isChecked() == true){
+                mHardKeyImage.setImageResource(R.drawable.hardkey_reverse_on);
+			}else{
+				mHardKeyImage.setImageResource(R.drawable.hardkey_reverse_off);
+			}
+        }else{
+			if(isChecked() == true){        
+               mHardKeyImage.setImageResource(R.drawable.hardkey_normal_on);
+			}else{
+				mHardKeyImage.setImageResource(R.drawable.hardkey_normal_off);
+			}
+        }	
+	  }
+    }
+
+	public void setOnHardKeyClickListener(HardKeySettingsEX.OnHardKeyClickListener listener){
+        Log.d(TAG, "setOnHardKeyClickListener " + listener);
+        mOnHardKeyClickListener = listener;
+	}
+
+    /**
+     * Process the check status change about the preference
+     * 
+     * @param buttonView
+     *            the buttonView which change the check status
+     * @param isChecked
+     *            the current check status
+     */
+    public void onCheckedChanged(CompoundButton buttonView, boolean isChecked) {
+        Log.d(TAG , "onCheckedChanged " + isChecked + getKey());
+
+        if (isChecked) {
+            if (sCurrentChecked != null) {
+                sCurrentChecked.setChecked(false);
+            }
+            sCurrentChecked = buttonView;
+        }
+    }
+
+    /**
+     * Return the preference current checked status
+     * 
+     * @return the current check status
+     */
+    public boolean isChecked() {
+        if (sActiveKey != null) {
+            return getKey().equals(sActiveKey);
+        }
+        return false;
+    }
+
+    /**
+     * When persist.sys.input.changemode is changed  , set it checked Called in
+     * HardKeySettingsEX.java
+     */
+    public void setChecked() {
+        sActiveKey = getKey();
+        if (mCheckboxButton != null) {
+            if (!mCheckboxButton.equals(sCurrentChecked)) {
+                if (sCurrentChecked != null) {
+                    sCurrentChecked.setChecked(false);
+                }
+                Log.d(TAG , "setChecked" + getKey());
+                mCheckboxButton.setChecked(true);
+                sCurrentChecked = mCheckboxButton;
+            }
+
+        } else {
+            Log.d(TAG , "mCheckboxButton is null");
+        }
+    }
+}
