diff --git a/alps/frameworks/native/services/inputflinger/InputDispatcher.cpp b/alps/frameworks/native/services/inputflinger/InputDispatcher.cpp
index 64b7a3d..79a1519 100755
--- a/alps/frameworks/native/services/inputflinger/InputDispatcher.cpp
+++ b/alps/frameworks/native/services/inputflinger/InputDispatcher.cpp
@@ -2550,12 +2550,31 @@ void InputDispatcher::notifyKey(const NotifyKeyArgs* args) {
             metaState &= ~AMETA_META_ON;
         }
     }
-
+	/*add by zhoudawei for back & menu swap 20140623 start*/
+    ALOGD("notifyKey -keyCode=0x%x, scanCode=0x%x",args->keyCode, args->scanCode);
+	int32_t tempKeyCode,tempScanCode;
+    tempKeyCode = args->keyCode;
+    tempScanCode = args->scanCode;
+	if(args->keyCode == 4 || args->keyCode == 82){
+			char buf[PROPERTY_VALUE_MAX];
+			property_get("persist.sys.input.changemode", buf, "0");
+		if (!strcmp(buf, "1")){
+			if(args->keyCode == 4 && args->scanCode == 158){
+				tempKeyCode = 82;/*KEY_MENU*/
+				tempScanCode = 139;
+			}else if(args->keyCode == 82 && args->scanCode == 139){
+				tempKeyCode = 4;/*KEY_BACK*/
+				tempScanCode = 158;
+			}
+		}
+	}
+    /*add by zhoudawei for back & menu swap 20140623 end*/
     KeyEvent event;
     event.initialize(args->deviceId, args->source, args->action,
-            flags, keyCode, args->scanCode, metaState, 0,
+            //flags, keyCode, args->scanCode, metaState, 0,
+			flags, tempKeyCode, tempScanCode, metaState, 0,
             args->downTime, args->eventTime);
-
+		/*modify by zhoudawei for back & menu swap 20140623 end*/  
     mPolicy->interceptKeyBeforeQueueing(&event, /*byref*/ policyFlags);
 
     bool needWake;
@@ -2574,11 +2593,13 @@ void InputDispatcher::notifyKey(const NotifyKeyArgs* args) {
         }
 
         int32_t repeatCount = 0;
+		/*modify by zhoudawei for back & menu swap 20140623 start*/
         KeyEntry* newEntry = new KeyEntry(args->eventTime,
                 args->deviceId, args->source, policyFlags,
-                args->action, flags, keyCode, args->scanCode,
+                //args->action, flags, keyCode, args->scanCode,
+				args->action, flags, tempKeyCode, tempScanCode,
                 metaState, repeatCount, args->downTime);
-
+		 /*modify by zhoudawei for back & menu swap 20140623 end*/
         needWake = enqueueInboundEventLocked(newEntry);
         mLock.unlock();
     } // release lock
