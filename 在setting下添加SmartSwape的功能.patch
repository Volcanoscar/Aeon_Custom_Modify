+
+public class HardKeySettingsEX extends SettingsPreferenceFragment {
+    private static final String TAG = "HardKeySettingsEX";
+
+    private static final String HARDKEY_NORMAL_KEY = "hardkey_normal_mode";
+    private static final String HARDKEY_REVERSAL_KEY = "hardkey_reversal_mode";
+
+    private HardKeyPreference activePreference;
+	
+    private HardKeyPreference normalmodePreference;
+    private HardKeyPreference reversalmodePreference;
+
+    /**
+     * called to do the initial creation of a fragment
+     * 
+     * @param icicle
+     */
+    public void onCreate(Bundle icicle) {
+        Log.d(TAG, "onCreate");
+        super.onCreate(icicle);
+
+        PreferenceScreen root = getPreferenceScreen();
+        if (root != null) {
+            root.removeAll();
+        }
+        addPreferencesFromResource(R.xml.hardkey_settingsex);
+
+		normalmodePreference = (HardKeyPreference) findPreference(HARDKEY_NORMAL_KEY);
+		normalmodePreference.setOnHardKeyClickListener(mHardKeySettingListener);
+		reversalmodePreference = (HardKeyPreference) findPreference(HARDKEY_REVERSAL_KEY);		
+		reversalmodePreference.setOnHardKeyClickListener(mHardKeySettingListener);		
+    }
+
+    /**
+     * Get the active profile from framework and update UI
+     */
+    private void updateActivePreference() {
+
+        String changemode = SystemProperties.get("persist.sys.input.changemode", "0");
+        Log.d(TAG,"hard key mode = " + changemode);
+        if (changemode != null && ("1".equals(changemode))) {
+               activePreference = reversalmodePreference;
+        }else{
+               activePreference = normalmodePreference;
+        }
+        if (activePreference != null) {
+            activePreference.setChecked();
+        }
+    }
+
+
+    /**
+     * called when the fragment is visible to the user Need to update summary
+     * and active profile, register for the profile change
+     */
+    public void onResume() {
+        super.onResume();
+
+        updateActivePreference();
+		dynamicShowImage();
+    }
+
+    /**
+     * called when the fragment is unvisible to the user unregister the profile
+     * change listener
+     */
+    public void onPause() {
+        super.onPause();
+    }
+
+    /**
+     * Click the preference and enter into the EditProfile
+     * 
+     * @param preferenceScreen
+     * @param preference
+     *            the clicked preference
+     * @return set success or fail
+     */
+    public boolean onPreferenceTreeClick(PreferenceScreen preferenceScreen,
+            Preference preference) {
+        if (preference instanceof HardKeyPreference) {
+        HardKeyPreference pref = (HardKeyPreference) preference;
+        String key = pref.getKey();
+        Log.d(TAG,"onPreferenceTreeClick key = " +key);
+        if (key != null && (HARDKEY_REVERSAL_KEY.equals(key))) {
+                SystemProperties.set("persist.sys.input.changemode", "1");
+        }else{
+                SystemProperties.set("persist.sys.input.changemode", "0");
+        }
+        pref.setChecked();
+		dynamicShowImage();
+        }
+        return super.onPreferenceTreeClick(preferenceScreen, preference);
+    }
+
+    public void dynamicShowImage(){
+		if(normalmodePreference!=null){
+            normalmodePreference.dynamicShowImage();
+		}
+		if(reversalmodePreference!=null){
+            reversalmodePreference.dynamicShowImage();
+		}		
+    }
+
+	/**
+	 * Interface definition for a callback to be invoked when hard key preference is clicked.
+	 */
+   public interface OnHardKeyClickListener {
+	/**
+	 * Called when hard key preference has been clicked.
+	 *
+	 * @param v The hard key preference that was clicked.
+	 */
+	public void onClick();
+   }
+   
+    protected int getMetricsCategory() {
+        return MetricsLogger.ACCOUNT;
+    }
+
+    private final OnHardKeyClickListener mHardKeySettingListener = new OnHardKeyClickListener() {
+        public void onClick() {
+            Log.d(TAG, "on Click Hard Key");
+			dynamicShowImage();
+        }
+    };
+	
+}
