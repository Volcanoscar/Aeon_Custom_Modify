+package com.android.settings;
+
+
+import android.preference.CheckBoxPreference;
+import android.preference.Preference;
+import android.preference.PreferenceScreen;
+import android.provider.Settings;
+import android.os.SystemProperties;
+import android.os.Bundle;
+import android.util.Log;
+import com.android.internal.logging.MetricsLogger;
+
+/**
+ * Hard Key settings.
+ */
+public class HardKeySettings extends SettingsPreferenceFragment {
+
+    private static final String HARDKEY_CATEGORY = "hardkey_category";
+    private static final String HARDKEY_DATA = "hardkey_data";
+    private static final String TAG = "HardKeySettings";
+        
+    private CheckBoxPreference mHardKey;
+
+    @Override
+    public void onCreate(Bundle savedInstanceState) {
+        super.onCreate(savedInstanceState);
+        addPreferencesFromResource(R.xml.hardkey_settings);
+        final PreferenceScreen screen = getPreferenceScreen();
+        mHardKey = (CheckBoxPreference) screen.findPreference(HARDKEY_DATA);
+        updateToggles();
+    }
+
+    @Override
+    public void onResume() {
+        super.onResume();
+        // Refresh UI
+        updateToggles();
+    }
+
+    @Override
+    public void onStop() {
+        super.onStop();
+    }
+
+    @Override
+    public boolean onPreferenceTreeClick(PreferenceScreen preferenceScreen,
+            Preference preference) {
+        if (preference == mHardKey) {
+            if (!mHardKey.isChecked()) {
+                SystemProperties.set("persist.sys.input.changemode", "0");
+                //mHardKey.setSummary(R.string.hardkey_normal_summary);
+                setHardKeySummary(false);
+            } else {
+                SystemProperties.set("persist.sys.input.changemode", "1");
+                setHardKeySummary(true);
+                //mHardKey.setSummary(R.string.hardkey_reverse_summary);
+           }
+
+        } 
+        return super.onPreferenceTreeClick(preferenceScreen, preference);
+    }
+    protected int getMetricsCategory() {
+        return MetricsLogger.ACCOUNT;
+    }
+    /*
+     *Set Hard Key summary
+    */
+    private void setHardKeySummary(boolean hardkeyEnable){
+        if(hardkeyEnable == true){
+                mHardKey.setSummary(R.string.hardkey_reverse_summary);
+        }else{
+                mHardKey.setSummary(R.string.hardkey_normal_summary);
+        }
+    }
+    /*
+     * Creates toggles 
+     */
+    private void updateToggles() {
+        //fix later:read from property
+        boolean hardkeyEnabled = false;
+        String changemode = SystemProperties.get("persist.sys.input.changemode", "0");
+        Log.d(TAG,"hard key mode = " + changemode);
+        if (changemode != null && ("1".equals(changemode))) {
+               hardkeyEnabled = true;
+        }
+        mHardKey.setChecked(hardkeyEnabled);
+        setHardKeySummary(hardkeyEnabled);
+    }
+}
